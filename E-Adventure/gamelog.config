<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Gamelog config</comment>
<entry key="tracking">disabled</entry>
<entry key="remote-service">disabled</entry>
<entry key="local-dump">enabled</entry>
<entry key="snapshots-capture">enabled</entry>
<entry key="interaction-log">enabled</entry>
<entry key="controller-class">es.eucm.eadventure.gamelog.prv.GameLogController</entry>
<entry key="snapshots-sample-freq">10000</entry>
<entry key="snapshots-send-freq">10000</entry>
<entry key="interaction-log-dump-freq">10000</entry>
<entry key="interaction-log-send-freq">10000</entry>
<entry key="interaction-log-ll-sample-freq">1000</entry>
</properties>